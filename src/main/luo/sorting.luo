private int array sort(int array t){
    for(int j = 0; j < length(t); j++)
        for(int i = 0; i < length(t) -1 - j; i++)
            if (t[i] > t[i+1]){
                int tmp = t[i];
                t[i] = t[j];
                t[j] = tmp;
            }
    return t
}

void main(char array array args)
{
    int array t = { 12, 22, 5, 1, 6}
    sort(t)
    for(int i : t) print("t[", i, "]=", t[i])
}